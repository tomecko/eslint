[project]
	description = A fully pluggable tool for identifying and reporting on patterns in JavaScript.
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/tomecko
[access "refs/for/refs/*"]
	read = group user/tomecko
	push = group user/tomecko
	label-Code-Review = -2..+2 group user/tomecko
	label-Verified = -1..+1 group user/tomecko
	rebase = group user/tomecko
	removeReviewer = group user/tomecko
	submit = group user/tomecko
[access "refs/heads/*"]
	read = group user/tomecko
	create = group user/tomecko
	push = +force group user/tomecko
	pushMerge = group user/tomecko
[access "refs/tags/*"]
	push = +force group user/tomecko
	pushTag = group user/tomecko
	pushSignedTag = group user/tomecko
